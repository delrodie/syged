{% extends 'base.html.twig' %}

{% block body %}
    <h1>Distributeur</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ distributeur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ distributeur.nom }}</td>
            </tr>
            <tr>
                <th>Situation</th>
                <td>{{ distributeur.situation }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ distributeur.contact }}</td>
            </tr>
            <tr>
                <th>Structure</th>
                <td>{{ distributeur.structure }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{% if distributeur.statut %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ distributeur.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ distributeur.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ distributeur.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ distributeur.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ distributeur.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('distributeur_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('distributeur_edit', { 'id': distributeur.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
